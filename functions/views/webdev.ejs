}

.team .card{
  width:20%;
  float: left;
  padding:0px 6px;
}
.team .card .box {
  padding:15px;
  background-color: #2d3035;
  border-radius:5px;
  transition: margin 1s ease,box-shadow 1s ease;
}
.team .card .box:hover{
  margin-top:-15px;
  box-shadow:0px 0px 40px black;
}
.team .card .box img{
  width:100px;
  display: block;
  margin:10px auto;
  border-radius:50%;

}

.team .card .box h4{
  font-size: 20px;
  font-family: open sans;
  margin:20px 0px 10px;
  color:#ea5e71;
  text-align: center;
  font-weight: 700;

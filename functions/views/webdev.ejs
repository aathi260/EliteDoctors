  font-size: 15px;
  font-family: open sans;
  font-weight: 400;
  line-height: 24px;
  text-align: center;
  padding:0px 20px;
}



/*responsive*/

@media(max-width: 991px){
  .team .card{
    width:50%;
    margin-bottom:30px;
  }
}

@media(max-width:550px){
